<h3>DispatchScheduleTable</h3>

<div class="table-wrapper">
    <table class="fl-table">
        <thead>
            <tr>
                <th>Task:Name</th>
                <th>Task:Description</th>
                <th>Time:Started</th>
                <th>Time:End</th>
                <th>Task:Status</th>
                <th>Task:Actions</th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < 10; i++)
            {
                <tr>
                    <td>Content @i</td>
                    <td>Content @i</td>
                    <td>Content @i</td>
                    <td>Content @i</td>
                    <td>Content @i</td>
                    <td>Content @i</td>
                </tr>
            }
        </tbody>
    </table>
</div>

